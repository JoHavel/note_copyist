mkdir out/zips/

# A
python3.11 generate_images_for_mashcima.py out/for_mashcima/A_72 MUSCIMA --seed 72
cd out/zips/ && zip -r ../../out/for_mashcima/A_72; cd ../../
python3.11 generate_images_for_mashcima.py out/for_mashcima/A_73 MUSCIMA --seed 73
cd out/zips/ && zip -r ../../out/for_mashcima/A_73; cd ../../
python3.11 generate_images_for_mashcima.py out/for_mashcima/A_74 MUSCIMA --seed 74
cd out/zips/ && zip -r ../../out/for_mashcima/A_73; cd ../../

# B
python3.11 generate_images_for_mashcima.py out/for_mashcima/B_72 REBELO --seed 72
cd out/zips/ && zip -r ../../out/for_mashcima/B_72; cd ../../
python3.11 generate_images_for_mashcima.py out/for_mashcima/B_73 REBELO --seed 73
cd out/zips/ && zip -r ../../out/for_mashcima/B_73; cd ../../
python3.11 generate_images_for_mashcima.py out/for_mashcima/B_74 REBELO --seed 74
cd out/zips/ && zip -r ../../out/for_mashcima/B_74; cd ../../

# C
python3.11 generate_images_for_mashcima.py out/for_mashcima/C_72 MUSCIMA REBELO --seed 72
cd out/zips/ && zip -r ../../out/for_mashcima/C_72; cd ../../
