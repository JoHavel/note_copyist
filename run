# MUSCIMA models
python3.11 padd_muscima.py downloaded/other MUSCIMA --seed 72
python3.11 main.py --seed 72 --directory out/models_for_mashcima/muscima72 --dataset other --cat onecat --layers --conv_layers 64 16 4 --stride 3 --kernel 5 --multiply_of 27 --batch 5 --network aae --epoch 150 --latent 4
mv downloaded/other downloaded/otherM72
python3.11 padd_muscima.py downloaded/other MUSCIMA --seed 73
python3.11 main.py --seed 73 --directory out/models_for_mashcima/muscima73 --dataset other --cat onecat --layers --conv_layers 64 16 4 --stride 3 --kernel 5 --multiply_of 27 --batch 5 --network aae --epoch 150 --latent 4
mv downloaded/other downloaded/otherM73
